	rem ---------------------------------------------------------------------------
	rem Force Field
	rem ---------------------------------------------------------------------------
	
	if Script( slot , lin ).cmd = CMD_CREATEFORCEFIELD
		uScript.LastIndex = CreateForceField( ScriptFloatData( ScriptDataList(slot,lin,0).Index ).dat# , ScriptFloatData( ScriptDataList(slot,lin,1).Index ).dat# , ScriptFloatData( ScriptDataList(slot,lin,2).Index ).dat# , ScriptIntegerData( ScriptDataList(slot,lin,3).Index ).dat )
