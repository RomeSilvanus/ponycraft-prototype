		GUIAddStaticFrame( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptStringData( ScriptDataList(slot,lin,1).Index ).dat$ , ScriptIntegerData( ScriptDataList(slot,lin,2).Index ).dat )
