		DestroyAllForceField()
		exitfunction 0
	endif
	
	rem ---------------------------------------------------------------------------
	rem GUI
	rem ---------------------------------------------------------------------------
	
	if Script( slot , lin ).cmd = CMD_GUIADDSTATICIMAGE
		uScript.LastIndex = GUIAddStaticImage( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,2).Index ).dat , ScriptStringData( ScriptDataList(slot,lin,3).Index ).dat$ )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIDESTROYELEMENT
		GUIDestroyElement( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIADDBUTTON
		uScript.LastIndex = GUIAddButton( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,2).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,3).Index ).dat , ScriptStringData( ScriptDataList(slot,lin,4).Index ).dat$ , ScriptStringData( ScriptDataList(slot,lin,5).Index ).dat$ , ScriptStringData( ScriptDataList(slot,lin,6).Index ).dat$ , ScriptStringData( ScriptDataList(slot,lin,7).Index ).dat$ , ScriptStringData( ScriptDataList(slot,lin,8).Index ).dat$ , ScriptIntegerData( ScriptDataList(slot,lin,9).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUILOCKBUTTON
		GUILockButton( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIUNLOCKBUTTON
		GUIUnlockButton( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUISHOWELEMENT
		GUIShowElement( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIHIDEELEMENT
		GUIHideElement( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIBUTTONENABLETOGGLE
		GUIButtonEnableToggle( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIBUTTONDISABLETOGGLE
		GUIButtonDisableToggle( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIBINDBUTTONTOUNIT
		GUIBindButtonToUnit( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIPOSITIONELEMENT
		GUIPositionElement( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,2).Index ).dat )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_GUIBUTTONEVENTCLICK
		
		rem find ID of GUI element
		for n = 0 to uGUI.CurrentMax
			if GUI( n ).ID = ScriptIntegerData( ScriptDataList(slot,lin,2).Index ).dat then exit
		next n
		if n = uGUI.CurrentMax + 1
			AddTextToConsole( 3 , "Unable to set event. Button ID " + chr$(34) + str$( ScriptIntegerData( ScriptDataList(slot,lin,2).Index ).dat ) + chr$(34) + "not found!" )
			exitfunction 0
		endif
		
		rem setup correctly
		GUI( n ).EventClickActive = 1
		GUI( n ).EventClickScript = ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat
		GUI( n ).EventClickLine = ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat+1
		exitfunction 0
	endif
