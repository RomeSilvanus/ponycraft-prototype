		SetAllLoggingFlags( 1 )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_TURNALLLOGGINGOFF
		SetAllLoggingFlags( 0 )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_CAMERALOGGING
		uCamera.logging = ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_CLICKOBJLOGGING
