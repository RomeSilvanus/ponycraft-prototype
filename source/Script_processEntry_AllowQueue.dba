function _Process_Command_Allow_Queue( slot , lin )
	
	rem ---------------------------------------------------------------------------
	rem Core Commands
	rem ---------------------------------------------------------------------------
	
	if Script( slot , lin ).cmd = CMD_OPENTOREAD
		OpenToRead( ScriptIntegerData(ScriptDataList(slot,lin,1).index).dat , ScriptStringData(ScriptDataList(slot,lin,0).index).dat$ )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_OPENTOWRITE	
		OpenToWrite( ScriptIntegerData(ScriptDataList(slot,lin,0).index).dat , ScriptStringData(ScriptDataList(slot,lin,1).index).dat$ )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_CLOSEFILE
		close file ScriptIntegerData(ScriptDataList(slot,lin,0).index).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_WRITEBYTE
		write byte ScriptIntegerData(ScriptDataList(slot,lin,0).index).dat , ScriptIntegerData(ScriptDataList(slot,lin,1).index).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_WRITELONG
		write long ScriptIntegerData(ScriptDataList(slot,lin,0).index).dat , ScriptIntegerData(ScriptDataList(slot,lin,1).index).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_WRITEFLOAT
		write float ScriptIntegerData(ScriptDataList(slot,lin,0).index).dat , ScriptFloatData(ScriptDataList(slot,lin,1).index).dat#
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_WRITESTRING
		write string ScriptIntegerData(scriptDataList(slot,lin,0).index).dat , ScriptStringData(ScriptDataList(slot,lin,1).index).dat$
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_WRITEFILEBLOCK
		write fileblock ScriptIntegerData(ScriptDataList(slot,lin,0).index).dat , ScriptStringData(ScriptDataList(slot,lin,1).index).dat$
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_READFILEBLOCK
		read fileblock ScriptIntegerData(ScriptDataList(slot,lin,0).index).dat , ScriptStringdata(ScriptDataList(slot,lin,1).index).dat$
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_DELETEFILE
		delete file ScriptStringData(ScriptDataList(slot,lin,0).index).dat$
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_CALLEVENT
		CallEvent( ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat+1 )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_SETVARIABLEINTEGERINTEGER
		ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat = ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_SETVARIABLEINTEGERFLOAT
		ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat = ScriptFloatData( ScriptDataList(slot,lin,1).Index ).dat#
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_SETVARIABLEFLOATINTEGER
		ScriptFloatData( ScriptDataList(slot,lin,0).Index ).dat# = ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_SETVARIABLEFLOATFLOAT
		ScriptFloatData( ScriptDataList(slot,lin,0).Index ).dat# = ScriptFloatData( ScriptDataList(slot,lin,1).Index ).dat#
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_SETVARIABLESTRING
		ScriptStringData( ScriptDataList(slot,lin,0).Index ).dat$ = ScriptStringData( ScriptDataList(slot,lin,1).Index ).dat$
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_INCREMENT_INTEGER_INTEGER
		inc ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_INCREMENT_FLOAT_INTEGER
		inc ScriptFloatData( ScriptDataList(slot,lin,0).Index ).dat# , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_INCREMENT_INTEGER_FLOAT
		inc ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptFloatData( ScriptDataList(slot,lin,1).Index ).dat#
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_INCREMENT_FLOAT_FLOAT
		inc ScriptFloatData( ScriptDataList(slot,lin,0).Index ).dat# , ScriptFloatData( ScriptDataList(slot,lin,1).Index ).dat#
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_DECREMENT_INTEGER_INTEGER
		dec ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_DECREMENT_FLOAT_INTEGER
		dec ScriptFloatData( ScriptDataList(slot,lin,0).Index ).dat# , ScriptIntegerData( ScriptDataList(slot,lin,1).Index ).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_DECREMENT_INTEGER_FLOAT
		dec ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat , ScriptFloatData( ScriptDataList(slot,lin,1).Index ).dat#
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_DECREMENT_FLOAT_FLOAT
		dec ScriptFloatData( ScriptDataList(slot,lin,0).Index ).dat# , ScriptFloatData( ScriptDataList(slot,lin,1).Index ).dat#
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_FORCEOUTPUT
		uConsole.ForceOutput = ScriptIntegerData( ScriptDataList(slot,lin,0).Index ).dat
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_NOP
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_PRINT
		if ScriptDataList( slot , lin , 0 ).DatType = SCRIPTDATA_STRING then AddTextToConsole( 3 , ScriptStringData( ScriptDataList(slot,lin,0).Index ).dat$ )
		if ScriptDataList( slot , lin , 0 ).DatType = SCRIPTDATA_FLOAT then AddTextToConsole( 3 , str$(ScriptFloatData( ScriptDataList(slot,lin,0).Index ).dat#) )
		if ScriptDataList( slot , lin , 0 ).DatType = SCRIPTDATA_INTEGER then AddTextToConsole( 3 , str$(ScriptintegerData( ScriptDataList(slot,lin,0).Index ).dat) )
		exitfunction 0
	endif
	if Script( slot , lin ).cmd = CMD_RESETGAMETIMER
