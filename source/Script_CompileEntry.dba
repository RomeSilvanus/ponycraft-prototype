	n = _ScriptCompileEntryClickObj( slot , lin )						: if n > -3 then exitfunction n
	n = _ScriptCompileEntryDistortion( slot , lin )						: if n > -3 then exitfunction n
	n = _ScriptCompileEntryGUI( slot , lin )								: if n > -3 then exitfunction n
	n = _ScriptCompileEntryMessage( slot , lin )							: if n > -3 then exitfunction n
	n = _ScriptCompileEntryMinimap( slot , lin )							: if n > -3 then exitfunction n
	n = _ScriptCompileEntryScript( slot , lin )							: if n > -3 then exitfunction n
	n = _ScriptCompileEntryUnit( slot , lin )								: if n > -3 then exitfunction n
	n = _ScriptCompileEntryWorld( slot , lin )							: if n > -3 then exitfunction n
