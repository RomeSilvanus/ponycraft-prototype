		if ConsoleCmd$( n ) = "trigger_type_timer" then ConsoleCmd$( n ) = str$( TRIGGER_TYPE_TIMER )
		
	next n
	
	rem compile sections
	n = _ScriptCompileEntryHelp( slot , lin )								: if n > -3 then exitfunction n
	n = _ScriptCompileEntryCore( inString$ , slot , lin ) 			: if n > -3 then exitfunction n
	n = _ScriptCompileEntryForceField( slot , lin )						: if n > -3 then exitfunction n
	n = _ScriptCompileEntryGameControls( slot , lin ) 					: if n > -3 then exitfunction n
	n = _ScriptCompileEntryLogging( slot , lin )							: if n > -3 then exitfunction n
