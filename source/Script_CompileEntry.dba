	n = _ScriptCompileEntryEyeGlow( slot , lin )							: if n > -3 then exitfunction n
	n = _ScriptCompileEntryTrigger( slot , lin )                   : if n > -3 then exitfunction n
	
	rem unable to compile entry
	AddTextToConsole( 3 , "Unknown Command : " + chr$(34) + string$ + chr$(34) )
