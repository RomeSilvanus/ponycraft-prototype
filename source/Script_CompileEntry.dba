		if ConsoleCmd$( n ) = "unitsound_meteor_shower" then ConsoleCmd$( n ) = str$( UNITSOUND_METEOR_SHOWER )
		
		rem triggers
		if ConsoleCmd$( n ) = "trigger_junction_or" then ConsoleCmd$( n ) = str$( TRIGGER_JUNCTION_OR )
		if ConsoleCmd$( n ) = "trigger_junction_and" then ConsoleCmd$( n ) = str$( TRIGGER_JUNCTION_AND )
