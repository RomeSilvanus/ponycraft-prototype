function CompileEntry( inString$ , slot , lin )
	
	rem local variables
	local a as integer
	local n as integer
	local s as integer
	local r as integer
	local SplitCount as integer
	local string$ as string
	
	rem remove any tabs or spaces in front of it
	while fast left$( inString$ , 1 ) = tab$() or fast left$( string$ , 1 ) = " "
		inString$ = fast right$( inString$ , len(inString$)-1 )
	endwhile
	
	rem logging
	if uScript.logging = 1 then AddTextToConsole( 1 , "Compile -- " + chr$(34) + inString$ + chr$(34) )
	
	rem prepare string before split
	string$ = fast lower$( inString$ )
	string$ = ScriptAddSpacesToString( string$ )
	
	rem split string into array
	split string string$ , " "
	SplitCount = range( split count() , 0 , ConsoleArgExtractMax )
	s = 0
	r = 0 : rem 1 if quotation is open
	for n = 1 to SplitCount
		
		rem add to list
		if r = 0
			ConsoleCmd$(s) = get split word$(n)
		else
			ConsoleCmd$(s) = ConsoleCmd$(s) + " " + get split word$(n)
		endif
		
		rem open quotation
		if fast left$( ConsoleCmd$(s) , 1 ) = chr$(34) then r = 1
		
		rem close quotation
		if fast right$( ConsoleCmd$(s) , 1 ) = chr$(34) then r = 0
		
		rem remove quotations
		if fast left$( ConsoleCmd$(s) , 1 ) = chr$(34) then ConsoleCmd$(s) = fast right$( ConsoleCmd$(s) , len(ConsoleCmd$(s))-1 )
		if fast right$( ConsoleCmd$(s) , 1 ) = chr$(34) then ConsoleCmd$(s) = fast left$( ConsoleCmd$(s) , len(ConsoleCmd$(s))-1 )
		
		rem handle quotations and comments
		if fast left$( ConsoleCmd$(s) , 2 ) = "//" then exit
		if ConsoleCmd$(s) <> "" and r = 0 then inc s
	next n
	
	rem quotation not closed
	if r = 1
		AddTextToConsole( 3 , "Irregular quote sign count!" )
		exitfunction -1
	endif

	rem clear array
	for n = s to ConsoleArgExtractMax
		ConsoleCmd$(n) = ""
	next n
	
	rem pre-process static values
	for n = 1 to SplitCount

		rem unit control
		if ConsoleCmd$( n ) = "unitcontrol_player" then ConsoleCmd$( n ) = str$( UNITCONTROL_PLAYER )
		if ConsoleCmd$( n ) = "unitcontrol_noone" then ConsoleCmd$( n ) = str$( UNITCONTROL_NOONE )
		if ConsoleCmd$( n ) = "unitcontrol_enemy" then ConsoleCmd$( n ) = str$( UNITCONTROL_ENEMY )
		
		rem unit abilities
		if ConsoleCmd$( n ) = "unitability_move" then ConsoleCmd$( n ) = str$( UNITABILITY_MOVE )
		if ConsoleCmd$( n ) = "unitability_stop" then ConsoleCmd$( n ) = str$( UNITABILITY_STOP )
		if ConsoleCmd$( n ) = "unitability_hold" then ConsoleCmd$( n ) = str$( UNITABILITY_HOLD )
		if ConsoleCmd$( n ) = "unitability_patrol" then ConsoleCmd$( n ) = str$( UNITABILITY_PATROL )
		if ConsoleCmd$( n ) = "unitability_attack" then ConsoleCmd$( n ) = str$( UNITABILITY_ATTACK )
		if ConsoleCmd$( n ) = "unitability_fly" then ConsoleCmd$( n ) = str$( UNITABILITY_FLY )
		if ConsoleCmd$( n ) = "unitability_land" then ConsoleCmd$( n ) = str$( UNITABILITY_LAND )
		if ConsoleCmd$( n ) = "unitability_magic_missile" then ConsoleCmd$( n ) = str$( UNITABILITY_MAGIC_MISSILE )
		if ConsoleCmd$( n ) = "unitability_teleport" then ConsoleCmd$( n ) = str$( UNITABILITY_TELEPORT )
		if ConsoleCmd$( n ) = "unitability_changeling_melee_attack" then ConsoleCmd$( n ) = str$( UNITABILITY_CHANGELING_MELEE_ATTACK )
		if ConsoleCmd$( n ) = "unitability_force_field" then ConsoleCmd$( n ) = str$( UNITABILITY_FORCE_FIELD )
		if ConsoleCmd$( n ) = "unitability_meteor_shower" then ConsoleCmd$( n ) = str$( UNITABILITY_METEOR_SHOWER )
		if ConsoleCmd$( n ) = "smoke_black" then ConsoleCmd$( n ) = str$( SMOKE_BLACK )
		if ConsoleCmd$( n ) = "smoke_white" then ConsoleCmd$( n ) = str$( SMOKE_WHITE )
		if ConsoleCmd$( n ) = "unitsound_select" then ConsoleCmd$( n ) = str$( UNITSOUND_SELECT )
		if ConsoleCmd$( n ) = "unitsound_annoyed" then ConsoleCmd$( n ) = str$( UNITSOUND_ANNOYED )
		if ConsoleCmd$( n ) = "unitsound_target" then ConsoleCmd$( n ) = str$( UNITSOUND_TARGET )
